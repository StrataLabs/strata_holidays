- model_class = ConsultantCustomerDestination
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:vacation_consultant_id) + ':'
  %br
  = @consultant_customer_destination.vacation_consultant_id
%p
  %strong= model_class.human_attribute_name(:customer_id) + ':'
  %br
  = @consultant_customer_destination.customer_id
%p
  %strong= model_class.human_attribute_name(:cust_iti_header_id) + ':'
  %br
  = @consultant_customer_destination.cust_iti_header_id

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), consultant_customer_destinations_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_consultant_customer_destination_path(@consultant_customer_destination), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), consultant_customer_destination_path(@consultant_customer_destination), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
