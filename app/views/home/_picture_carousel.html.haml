.box
  .content{:style => 'background:#ffeb99'}
    #myCarousel.carousel.slide
      %ol.carousel-indicators
        %li.active{"data-slide-to" => "0", "data-target" => "#myCarousel"}
        %li{"data-slide-to" => "1", "data-target" => "#myCarousel"}
        %li{"data-slide-to" => "2", "data-target" => "#myCarousel"}
      / Carousel items
      .carousel-inner
        .active.item
          %img{:src => 'http://dev.justbooksclc.com.s3.amazonaws.com/strata_holidays/points_of_attractions/images/000/000/001/thumb/amritsar.jpg?1379423772'}
